{
    "name": "geolocator-monorepo",
    "workspaces": [
        "backend",
        "frontend"
    ],
    "scripts": {
        "start:backend": "npm run start --workspace backend",
        "start:frontend": "npm run start --workspace frontend",
        "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
